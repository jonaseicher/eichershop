{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "s": "vue-cli-service lint && vue-cli-service serve --port 8181",
    "serve": "vue-cli-service serve --port 8181",
    "builddefault": "vue-cli-service build",
    "build1": "vue-cli-service build --target wc --name eichershop './src/*.vue'",
    "build": "vue-cli-service build --target wc --inline-vue --name eichershop './src/*.vue'",
    "dev": "vue-cli-service build --target wc --name eichershop './src/*.vue' --mode development",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/vue-cli-service test:unit --runInBand",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@material/mwc-icon": "^0.20.0",
    "@material/mwc-textfield": "^0.20.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuex": "^3.6.2",
    "vuex-class-modules": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-airbnb": "~5.3.0",
    "@vue/eslint-config-typescript": "~7.0.0",
    "@vue/test-utils": "^1.1.2",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.5.0",
    "git-describe": "^4.0.4",
    "jest-mock-axios": "^4.2.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "typescript": "~4.1.3",
    "vue-jest": "^5.0.0-0",
    "vue-template-compiler": "^2.6.12"
  }
